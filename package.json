{
    "author": "kai zhu <kaizhu256@gmail.com>",
    "description": "api documentation for [notie (v4.3.0)](https://jaredreich.com/projects/notie) [![npm package](https://img.shields.io/npm/v/npmdoc-notie.svg?style=flat-square)](https://www.npmjs.org/package/npmdoc-notie) [![travis-ci.org build-status](https://api.travis-ci.org/npmdoc/node-npmdoc-notie.svg)](https://travis-ci.org/npmdoc/node-npmdoc-notie)",
    "devDependencies": {
        "electron-lite": "kaizhu256/node-electron-lite#alpha",
        "notie": "*",
        "utility2": "kaizhu256/node-utility2#alpha"
    },
    "engines": {
        "node": ">=4.0"
    },
    "homepage": "https://github.com/npmdoc/node-npmdoc-notie",
    "keywords": [
        "documentation",
        "notie"
    ],
    "license": "MIT",
    "main": "lib.npmdoc_notie.js",
    "name": "npmdoc-notie",
    "nameAlias": "npmdoc_notie",
    "nameOriginal": "npmdoc-notie",
    "os": [
        "darwin",
        "linux"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/npmdoc/node-npmdoc-notie.git"
    },
    "scripts": {
        "build-ci": "utility2 shReadmeTest build_ci.sh",
        "env": "env",
        "heroku-postbuild": "npm install \"kaizhu256/node-utility2#alpha\" && utility2 shDeployHeroku",
        "postinstall": "[ ! -f npm_scripts.sh ] || ./npm_scripts.sh postinstall",
        "start": "PORT=${PORT:-8080} utility2 start test.js",
        "test": "(set -e; export PORT=$(utility2 shServerPortRandom); utility2 test test.js)"
    },
    "version": "0.0.1"
}
